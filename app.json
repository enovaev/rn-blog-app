{
  "expo": {
    "name": "Blog app",
    "slug": "rn-blog-app",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/blog-app.png",
    "splash": {
      "image": "./assets/blog-app.png",
      "resizeMode": "cover",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enovaev.blog-app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.enovaev.blog-app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
